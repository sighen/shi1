    "def gcd(x,y):\n",
    "    if x<y:\n",
    "        m=x\n",
    "    else:\n",
    "        m=y\n",
    "    for i in range(m,0,-1):\n",
    "        if x%i==0 and y%i==0:\n",
    "            result = i\n",
    "            break\n",
    "    return result\n",
    "\n",
    "print(gcd(3,6))\n",
    "print(gcd(8,12))\n",
    "print(gcd(18,12))"
   ]
